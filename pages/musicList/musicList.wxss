/* pages/musicList/musicList.wxss */
page{
  width: 100%;
  height: 100%;
  background: #1a1a1a;
  box-sizing: border-box;
  padding-left: 25rpx;
}
.scroll-view-item{
  padding-left: 15rpx;
  color: white;
  line-height: 80rpx;
  height: 80rpx;
  font-size: 30rpx;
  border-bottom: 1rpx solid #313131;
}
.scroll-view-item text:nth-of-type(1){
  padding-right: 25rpx;
}
.active{
  color: #99aad8;
}